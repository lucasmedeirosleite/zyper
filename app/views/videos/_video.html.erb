<li class="collection-item avatar">
  <img src="<%= video.thumbnail %>" 
       alt="<%= video.title %>" 
       class="circle">
  <span class="title"><%= video.title %></span>
  <%= link_to video_path(id: video.id), class: 'secondary-content' do %>
    <i class="material-icons">send</i>
  <% end %>
</li>
